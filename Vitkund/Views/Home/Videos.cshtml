                    <div class="advance-tab-button">
                        <ul class="nav nav-tabs tab-button-style-2 lisul" id="myTab-4" role="tablist">
                            @if (ViewBag.tblchapters != null)
                            {

                                foreach (var item in ViewBag.tblchapters)
                                {
                                    <li role="presentation">
                                        <a href="#" class="tab-button" id="home-tab-@item.Id" data-bs-toggle="tab" data-bs-target="#home-@item.Id" role="tab" aria-controls="home-4" aria-selected="false">
                                            <span class="title"> @item.Name</span>
                                        </a>
                                    </li>
                                }
                            }

                        </ul>
                    </div>

                    <div class="tab-content advance-tab-content-style-2">
                        @if (ViewBag.tblchapters != null)
                        {
                            foreach (var item in ViewBag.tblchapters)
                            {
                                <div class="tab-pane" id="home-@item.Id" role="tabpanel" aria-labelledby="home-tab-@item.Id">
                                    <div class="content">

                                        <div class="row g-5 align-items-center" style="padding-top:30px; padding-bottom:50px;">
                                            @if (Model != null)
                                            {
                                                foreach (var item2 in Model.Where(x => x.fK_Chapter == @item.Id))
                                                {
                                                    <div class="col-lg-3">
                                                        <div class="video-popup-wrapper">
                                                            @if (item2.VideoImage != null)
                                                            {


                                                                if (item2.VideoImage.Contains("Content"))
                                                                {
                                                            <img class="w-100 rbt-radius" src="@item2.VideoImage" alt="Video Images">
                                                                }
                                                                else
                                                                {
                                                            <img class="w-100 rbt-radius" src="~/Content/Uploads/@item2.VideoImage" alt="Video Images">

                                                                }
                                                            if (item2.VideoFile.Contains("Content"))
                                                            {
                                                                <a class="rbt-btn rounded-player popup-video position-to-top rbtplayer" href="@item2.VideoFile" style="max-width:50px; max-height:50px;">

                                                                    <span><i class="feather-play"></i></span>
                                                                </a>
                                                            }
                                                            else
                                                            {
                                                                <a class="rbt-btn rounded-player popup-video position-to-top rbtplayer" href="~/Content/Uploads/@item2.VideoFile" style="max-width:50px; max-height:50px;">
                                                                    <span><i class="feather-play"></i></span>
                                                                </a>
                                                            }
                                                            }

                                                            <h6 class="title" style="padding-top:10px;">@item2.FileTitle</h6>
                                                        </div>
                                                    </div>

                                                }
                                            }
                                        </div>


                                    </div>
                                </div>

                            }
                        }
